void makeTurn(double x, double y, double limit)
{
  if (x < .25)
  {
    forward();
  }
  else if (x<limit-.25)
  {
    back();
  }
  else if (y < .25)
  {
    right();
  }
  else if (y<limit-.25)
  {
    left();
  }
  else
  {
    funky();
  }
}

void funky()
{
  int rN = random(1,4);
  if (rN == 1)
  {
    forward();
    delay(100);
  }
  else if (rN == 2)
  {
    back();
    delay(100);
  }
  else if (rN == 3)
  {
    right();
    delay(100);
  }
  else if (rN == 4)
  {
    left();
    delay(100);
  }
  
}

void forward()
{
  one.write(135);
  two.write(45);
  three.write(95);
  four.write(95);
  delay(1);
}

void back()
{
  one.write(45);
  two.write(135);
  three.write(95);
  four.write(95);
  delay(1);
}

void right()
{
  one.write(95);
  two.write(95);
  three.write(45);
  four.write(135);
  delay(1);
}

void left()
{
  one.write(95);
  two.write(95);
  three.write(135);
  four.write(45);
  delay(1);
}
